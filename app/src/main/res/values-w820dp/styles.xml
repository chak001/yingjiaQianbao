    <style name="HomepageTableStlye">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">@dimen/tablelayout_paddingBottom</item>
        <item name="android:paddingLeft">@dimen/tablelayout_paddingLeftorRight</item>
        <item name="android:paddingRight">@dimen/tablelayout_paddingLeftorRight</item>
        <item name="android:shrinkColumns">*</item>

    </style>

    <style name="VerticalLineStyle">
    <item name="android:layout_width">6px</item>
    <item name="android:layout_height">@dimen/sixty_height</item>
