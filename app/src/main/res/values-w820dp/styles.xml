    <style name="editlayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">65px</item>
        <item name="android:layout_marginTop">@dimen/edit_marginTop</item>
    </style>
    <style name="LoginEditStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">@dimen/login_layout_text_size</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:textColorHint">@color/hinttext</item>
    </style>
