<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Base application theme. -->

    <style name="ImageTranslucentTheme" parent="AppTheme"/>
    <style name="TranslucentTheme" parent="AppTheme"/>
    <style name="BaseAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/teachmenu</item>

        <item name="colorAccent">@color/perfectinfo_notice_pink</item>
    </style>

    <style name="AppTheme" parent="@style/BaseAppTheme">
        <item name="android:listDivider">@drawable/albumdivider</item>
    </style>
    <style name="ToolBarWithNavigationBack">
        <item name="navigationIcon">@drawable/icon_back</item>
    </style>
    
    <style name="ToorBarTheme" parent="@style/ThemeOverlay.AppCompat.ActionBar">
        <item name="actionMenuTextColor">@color/white</item>
        <item name="android:textSize">16sp</item>
    </style>
    
    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="colorAccent">@color/main_checked_txt</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="mProgress_circle">
        <item name="android:indeterminateDrawable">@drawable/progressbar_circle</item>
        <item name="android:minWidth">25dp</item>
        <item name="android:minHeight">25dp</item>
        <item name="android:maxWidth">60dp</item>
        <item name="android:maxHeight">60dp</item>
    </style>



